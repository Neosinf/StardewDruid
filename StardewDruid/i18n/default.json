    // DialogueScene
    "DialogueScene.approachEffigy.1.0": "Farmer",
    "DialogueScene.approachEffigy.2.0": "You come at last",
    "DialogueScene.approachEffigy.3.0": "I'm in the ceiling",
    "DialogueScene.approachEffigy.4.0": "Stand here and perform the rite",
